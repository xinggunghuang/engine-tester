    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Engine Tester",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "engine_tester.server:app",
                "--host=0.0.0.0",
                "--port=8000"
            ],
            "jinja": true,
            "env": {
                "ENGINE_TESTER_ROOT": "${workspaceFolder}"
            },
            "console": "integratedTerminal"
